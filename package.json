    "react-dom": "^0.14.6",
    "react-mixin": "^3.0.3",
    "react-radio-group": "^2.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "react-select": "^0.9.1",
    "redux": "^3.5.2",
    "redux-localstorage": "^0.4.0",
    "redux-promise": "^0.5.1",
    "redux-thunk": "^2.1.0",
    "source-map-support": "^0.4.0",
